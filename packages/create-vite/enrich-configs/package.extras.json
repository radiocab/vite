{
  "name": "vite-scaffolded-from-template",
  "description": "ViteJS starter template with some basic dependencies, enriched with usefull plugins.",
  "version": "0.0.1",
  "type": "module",
  "keywords": [
    "template",
    "boilerplate",
    "boilerplate-template",
    "postcss",
    "eslint",
    "prettier",
    "scaffold",
    "scaffold-template",
    "vite",
    "vitejs",
    "vite-template",
    "vitejs-template"
  ],
  "scripts": {
    "dev-vite": "vite --host --open",
    "lint-eslint": "eslint --ext .js ./",
    "lint:fix-eslint": "eslint --fix --ext .js ./",
    "format-prettier": "prettier --write \"./**/*.{js,jsx,json,css}\"",
    "build-vite": "vite build",
    "preview-vite": "vite preview --open",
    "buildpreview-vite": "vite build && vite preview --open"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.21",
    "cssnano": "^7.0.7",
    "eslint": "^8.57.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.4.1",
    "postcss": "^8.5.4",
    "postcss-nesting": "^13.0.1",
    "prettier": "^3.5.3",
    "vite": "6.3.5",
    "vite-plugin-eslint": "^1.8.1"
  },
  "dependencies": {
    "the-new-css-reset": "^1.11.3"
  }
}